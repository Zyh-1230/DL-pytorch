   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "44b6fc21-3c06-47b0-88ec-27e45492d78a",
   "metadata": {},
   "outputs": [],
   "source": [
    "y = np.array([1, 0, 0])\n",
    "z = np.array([0.2, 0.1, -0.1])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "d250f363-d9f8-4b6c-9338-9877ff166fae",
   "metadata": {},
   "outputs": [],
   "source": [
    "y_pred = np.exp(z) / np.exp(z).sum() # Softmax layer\n",
    "loss = (- y * np.log(y_pred)).sum()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "id": "88e7a606-5321-4c43-9f9e-bb94c4725a12",
   "metadata": {},
